pipeline {
    agent any

    stages {
        stage('Build') {
           
            steps {
               sh 'sudo yum install maven -y'
            }
        }
        stage('demo'){
        
            steps   {
                sh 'rm -rf /j'
            }
        }
        stage('test')  {
            when{
                    branch 'dev'
            }
            steps {
                echo 'test'
            }
        }
    }
}
